{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "implicit-arrow-linebreak":"off",
        "function-paren-newline":"off",
        "no-console":"off",
        "operator-linebreak": ["error", "after"],
        "object-curly-newline": ["error", {"multiline": true}]

    }
}
